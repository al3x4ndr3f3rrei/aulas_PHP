<?php
//Variavel String - texto
$a = "teste";
//Variavel int _ numero inteiro 
$b = 123;
//Variavel float(ponte flutuante) - numero com ponto 
$c = 3.15;
//Variavel array(vetor) - variavel com varias posicoes
$d = false;
//varaivel array(vetor)  - variavel com varias posicoes
$e = array)"Alex",123,1.25);
//Mostrando os valores na tela 
echo var_dump($a)."<br>";
echo var_dump($b)."<br>";
echo var_dump($c)."<br>";
echo var_dump($d)."<br>";  
echo var_dump($e)."<br>";